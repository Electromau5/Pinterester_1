b <div class ="jumbotron">
<h1>Listing pins</h1>

<table>
  <thead>
    <tr>
      <div class="panel-heading">
      <th>Description</th>
      <th colspan="3"></th>
      <th>Options</th>
    </tr>

  </thead>
</div>
  <tbody>
    <% @pins.each do |pin| %>
      <tr>
        <td><%= pin.description %></td>
        <td><%= link_to 'Show', pin,class: "btn btn-success btn-lg" %></td>
        <td><%= link_to 'Edit', edit_pin_path(pin),class: "btn btn-success btn-lg" %></td>
        <td><%= link_to 'Delete', pin, method: :delete, data: { confirm: 'Are you sure?' },class: "btn btn-success btn-lg" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<div class="panel-footer">
<%= link_to 'New Pin', new_pin_path %>
</div>
</div>